local pp = Instance.new("Part")
nam = game.Players.LocalPlayer.Name
local starterGui = game:GetService("StarterGui")
pp.Parent = workspace
pp.Anchored = true
pp.Position = Vector3.new(2948.31, 831.4, -720.93)
pp.Size = Vector3.new(667.43, 0.09, 714.8)
game.Players.LocalPlayer.Character:MoveTo(pp.Position)
starterGui:SetCore("SendNotification", {
    Title = "press E to cat",
    Text = "Reign hell upon worlds",
    Icon = "rbxassetid://8645011774"
})

game:GetService('RunService').Stepped:connect(function()
for a, b in pairs(game.Workspace:GetChildren()) do
if b.Name == nam then
for i, v in pairs(game.Workspace[nam]:GetChildren()) do
if v:IsA("BasePart") then
v.CanCollide = false

end
end
end
end
end)

Workspace[nam].Head.Changed:connect(function()
for a, b in pairs(game.Workspace:GetChildren()) do
if b.Name == nam then
for i, v in pairs(game.Workspace[nam]:GetChildren()) do
if v:IsA("BasePart") then
v.CanCollide = false

end
end
end
end
end)
